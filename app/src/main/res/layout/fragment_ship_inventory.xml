<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="org.helpingkidsroundfirst.hkrf.navigation_bar_activities.inventory.ship_inventory.ShipInventoryFragment">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">

        <TextView
            android:id="@+id/textView37"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/ship_inventory_header" />

        <Button
            android:id="@+id/ship_inventory_button_add"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/ship_inventory_list" />

        <Button
            android:id="@+id/ship_inventory_button_submit"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/ship_inventory_submit" />
    </LinearLayout>

</FrameLayout>
